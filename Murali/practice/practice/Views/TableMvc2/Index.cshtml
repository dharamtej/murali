@model IEnumerable<practice.Models.TableMvc2>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PhoneNumber)
            </th>
            <th style="width:100px;height:100px;">
                @Html.DisplayNameFor(model => model.Loc)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Loc)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AutoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AutoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AutoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
